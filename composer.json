{
    "name": "awuniversity/oauth2-client-bundle",
    "type": "symfony-bundle",
    "description": "Integration with awuniversity/oauth2-client & awuniversity/oauth2-aw",
    "keywords": [],
    "homepage": "http://awuniversity.pl",
    "license": "MIT",
    "authors": [
        {
            "name": "Jurek"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": ">=4.3",
        "symfony/dependency-injection": ">4.3",
        "symfony/routing": ">=4.3",
        "symfony/http-foundation": ">=4.3",
        "awuniversity/oauth2-client": ">=1.0",
        "awuniversity/oauth2-aw": ">=1.0"
    },
    "autoload": {
        "psr-4": { "AwU\\OAuth2ClientBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "AwU\\OAuth2ClientBundle\\Tests\\": "tests/src/" }
    },
    "require-dev": {
    },
    "suggest": {
        "symfony/security-guard": "Integration with Symfony's Guard Security layer"
    }
}
